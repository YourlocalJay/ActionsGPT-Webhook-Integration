info:\
  title: ActionsGPT to Make Webhook API\
  description: An API to send task data to the Make webhook for Amazing Marvin integration\
  version: 1.0.0\
servers:\
  - url: https://hook.us2.make.com/b7psq55rtmlyco3sl8cwfjm4dsdyf7j6\
    description: Make Webhook URL\
paths:\
  /send-task:\
    post:\
      operationId: sendTask\
      summary: Send task data to the Make webhook\
      requestBody:\
        required: true\
        content:\
          application/json:\
            schema:\
              type: object\
              properties:\
                TaskName:\
                  type: string\
                  description: The name of the task to be created in Amazing Marvin\
                Category:\
                  type: string\
                  description: The category for the task (e.g., Learning & Certifications)\
                Priority:\
                  type: string\
                  description: The priority level of the task (e.g., High, Medium, Low)\
                DueDate:\
                  type: string\
                  description: The due date for the task (e.g., 'Friday')\
      responses:\
        '200':\
          description: Task data sent successfully}